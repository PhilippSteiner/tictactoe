<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.steiner_consult.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.steiner_consult.tictactoe.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.steiner_consult.tictactoe.SinglePlayerActivity"
            android:label="@string/app_name"/>
        <activity
            android:name="com.steiner_consult.tictactoe.MultiplayerActivity"
            android:label="@string/app_name"/>
    </application>

</manifest>
